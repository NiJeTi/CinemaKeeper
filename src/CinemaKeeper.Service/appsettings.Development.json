{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/service.log",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 1048576,
          "retainedFileCountLimit": 10
        }
      }
    ]
  },
  "DiscordBot": {
    "Prefix": "!"
  }
}
